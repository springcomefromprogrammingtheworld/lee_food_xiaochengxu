@import "../../lib/font/iconfont.wxss";
.jz_scrollbar {
  overflow: hidden;
  position: relative;
  height: 100%;
}
.jz_scrollbar__wrap {
  position: relative;
  height: 100%;
  /* overflow: scroll; */
}
.jz_scrollbar__resize {
  position: relative;
}
.jz_scrollbar__view {
  position: relative;
}
.placeOrder_productCategory {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  padding: 0;
  width: 100%;
  height: 100%;
  position: relative;
  padding-bottom: 56px;
}
.placeOrder_productCategory .fk_product_category_productContent_wrap {
  padding-top: 8px;
}
.placeOrder_productCategory::-webkit-scrollbar {/*滚动条整体样式*/
  width: 1px;     /*高宽分别对应横竖滚动条的尺寸*/
  height: 1px;
}
.placeOrder_productCategory::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
  border-radius: 10px;
  -webkit-box-shadow: inset 0 0 5px hsla(0, 8%, 95%, 0.87);
  background: #faf6f6;
}
.placeOrder_productCategory::-webkit-scrollbar-track {/*滚动条里面轨道*/
  -webkit-box-shadow: inset 0 0 5px rgba(240, 238, 238, 0.2);
  border-radius: 10px;
  background: #f8f5f5;
}
.placeOrder_productCategory .category{
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin: 0px 13px 0px 13px;
  border-bottom: 1px solid #e2e2e2;
}
.placeOrder_productCategory .categoryNames {
  color: #333;
  font-size: 14px;
  display: inline-block;
  width: 50%;
  text-align: left;
  margin: 13px 0 13px 0;
}
.placeOrder_productCategory .subtotal {
  color: #333;
  font-size: 14px;
  display: inline-block;
  width: 50%;
  text-align: left;
  margin: 13px 0 13px 0;
  text-align: right;
}
/* .placeOrder_productCategory .categoryBlock {
  margin-bottom: 60px;
} */
.placeOrder_productCategory .productCode{
  color: #333;
  font-size: 14px;
  padding-left: 5px;
  margin: 8px 0 0 8px;
}
.placeOrder_productCategory .orderBr{
  height: 10px;
  width: 100%;
  background-color: #e2e2e2;
}
.placeOrder_productCategory .total{
  color: #333;
  font-size: 14px;
  padding-left: 5px;
  margin: 0 0 8px 8px;
}
.placeOrder_productCategory .categoryBlock::-webkit-scrollbar {/*滚动条整体样式*/
  width: 1px;     /*高宽分别对应横竖滚动条的尺寸*/
  height: 1px;
}
.placeOrder_productCategory .categoryBlock::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
  border-radius: 10px;
  -webkit-box-shadow: inset 0 0 5px hsla(0, 8%, 95%, 0.87);
  background: #faf6f6;
}
.placeOrder_productCategory .categoryBlock::-webkit-scrollbar-track {/*滚动条里面轨道*/
  -webkit-box-shadow: inset 0 0 5px rgba(240, 238, 238, 0.2);
  border-radius: 10px;
  background: #f8f5f5;
}
.placeOrder_productCategory .productItem {
  background-color: #fff;
  margin: 8px;
  padding: 5px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  cursor: pointer;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  flex-wrap: nowrap
}
.placeOrder_productCategory .cartHeight{
  position: relative;
  max-height: 100vh;
  overflow: scroll;
}
.placeOrder_productCategory .cartBr{
  height: 1px;
  margin: 0px 13px 0px 13px;
  border-bottom: 1px solid #e2e2e2;
}
.placeOrder_productCategory .productItem .productPic {
  height: 150rpx;
  width: 150rpx;
  line-height: 150rpx;
  text-align: center;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  border: 1px solid #eee;
  overflow: hidden;
  position: relative;
}
.placeOrder_productCategory .productItem .productPic .productImg {
  width: 100%;
  height: 100%;
}
.placeOrder_productCategory .productItem .productProp {
  margin-left: 9px;
  vertical-align: top;
  margin-left: 0;
  -webkit-box-sizing: border-box;
  padding-right: 0;
  padding-bottom: 0;
  position: relative;
  box-sizing: border-box;
  height: 150rpx;
}
.placeOrder_productCategory .productItem .productProp .prop1 {
  color: #353535;
  font-size: 14px;
  overflow: hidden;
  padding-left: 9px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
.placeOrder_productCategory .productItem .productProp .prop2 {
  font-size: 15px;
  margin-left: 5px;
  margin-top: 15px;
  overflow: hidden;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  color: #353535
}
.placeOrder_productCategory .productItem .prop2 .price {
  font-size: 15px;
  overflow: hidden;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  color: #353535
}
.placeOrder_productCategory .productItem .prop2 .quantity {
  font-size: 14px;
  overflow: hidden;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  color: #353535;
  margin-left: 15px;
}
.placeOrder_productCategory .productItem .paymode {
  vertical-align: top;
  -webkit-box-sizing: border-box;
  padding-right: 0;
  padding-bottom: 0;
  position: relative;
  box-sizing: border-box;
  height: 60px;
  text-align: right;
}
.placeOrder_productCategory .productItem .paymode .paymode-title{
  font-size: 14px;
  overflow: hidden;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  color: #353535
}

/* 底部菜单样式 */
.nav {
  width: 100%;
  height: 50px;
  position: fixed;
  bottom: 0;
  background: #fff;
  border-top: 1px solid #eee;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: -ms-flexbox;
  -ms-flex-direction: row;
  -ms-flex-pack: justify;
  display: -webkit-box;
  display: flex;
  -ms-flex-pack: distribute;
  justify-content: space-around;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  z-index: 4;
}
.nav .nav-item {
  cursor: pointer;
  text-decoration: none;
  text-align: center;
  display: -ms-inline-flexbox;
  -ms-flex-direction: column;
  color: #666;
}
.nav .nav-icon-wrap {
  width: 26px;
  height: 26px;
  display: block;
  margin: 3px auto 0;
}
.nav .nav-icon-wrap.not_default {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
.nav .nav-text, .nav-2 .nav-text {
  max-width: 70px;
  font-size: 12px;
  font-weight: bold;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.nav .icon-active,.nav .nav-text-active{
  color: #1b1b1b;
}